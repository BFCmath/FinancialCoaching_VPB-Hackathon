# Environment Configuration for Insight Generator Agent
# Copy this file to .env and fill in your values

# Required: Google AI API Key for Gemini LLM
GOOGLE_API_KEY=your_google_api_key_here

# LLM Configuration
MODEL_NAME=gemini-2.5-flash-lite-preview-06-17
LLM_TEMPERATURE=0.1

# Transaction Processing Limits
MAX_TRANSACTIONS=200
DEFAULT_TRANSACTION_LIMIT=50
MAX_INSIGHT_LENGTH=2000

# Feature Toggles
ENABLE_PATTERN_ANALYSIS=true
ENABLE_BEHAVIORAL_INSIGHTS=true
ENABLE_STATISTICAL_ANALYSIS=true

# Development Settings
DEBUG_MODE=false
MOCK_DATA_MODE=true

# Example values for testing (optional)
# DEBUG_MODE=true
# MOCK_DATA_MODE=true
# LLM_TEMPERATURE=0.2
# MAX_TRANSACTIONS=100